<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.practice.practicespringboot.mapper.MemberMapper">
	<select id="selectAllMembers" resultType="com.practice.practicespringboot.dto.MemberDTO">
       SELECT ID, 
	       MEM_ID, 
	       MEM_NAME, 
	       MEM_PASSWORD, 
	       MEM_NICK,
	       MEM_EMAIL, 
	       MEM_CREATEAT
       FROM "MEMBER"
    </select>
</mapper>